<div class="center panel-body panel">
  <% if survey.picture_url %>
    <img id='survey-image' src="<%= survey.picture_url %>" alt="picture of <%= survey.title %>">
  <% end %>
  <h3><%= survey.title %> - (Your Answers)</h3>
  <ul>
    <% survey.questions.each do |q| %>
      <h4><%= q.question %></h4>
      <% if match_question_answer(completion, q) %>
        <li><em><%= match_question_answer(completion, q).choice %></em></li>
      <% end %>
    <% end %>
  </ul>
</div>